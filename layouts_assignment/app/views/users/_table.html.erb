<h1>All Users</h1>
<table class="table table-striped">
	<thead>
		<tr>
			<th>First Name</th>
			<th>Last Name</th>
			<th>Favorite Language</th>
		</tr>
	</thead>
	<tbody>
		<% @users.each do |user|%>
			<tr>
				<td><%= user.first_name %></td>
				<td><%= user.last_name %></td>
				<td><%= user.favorite_language %></td>
			</tr>
		<%end%>
	</tbody>
</table>